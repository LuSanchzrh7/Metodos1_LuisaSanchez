      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import sympy as sym"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "x = sym.Symbol('x',Real=True)\n",
        "func = 1/(1+ sym.exp(-0.5*(x**2))**(1/2))\n",
        "h = 0.05\n",
        "f= sym.lambdify([x],func,'numpy')"
      ],
      "metadata": {
        "id": "Dq2JvrZblh-O"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "d = np.linspace(-10,10)\n",
        "\n",
        "def D1(f,h):\n",
        "  M = [1,0,-1]\n",
        "  Derivada = []\n",
        "  Derivada.append(0)\n",
        "  i = 0\n",
        "  while(i + 2 != len(f)-1):\n",
        "    data = (M[0]*f[i] + M[1]*f[i+1] + M[2]*f[i+2])/2*h\n",
        "    Derivada.append(data)\n",
        "    i+=1\n",
        "  Derivada.append(0)\n",
        "\n",
        "  return Derivada\n",
        "\n",
        "print(\"Primera Derivada = \",D1(f(d),h))\n",
        "\n",
        "def D2(f,h):\n",
        "  M = [1,-2,1]\n",
        "  Derivada = []\n",
        "  Derivada.append(0)\n",
        "  i = 0\n",
        "  while(i + 2 != len(f)-1):\n",
        "    data = (M[0]*f[i] + M[1]*f[i+1] + M[2]*f[i+2])/2*h\n",
        "    Derivada.append(data)\n",
        "    i+=1\n",
        "  Derivada.append(0)\n",
        "\n",
        "  return Derivada\n",
        "\n",
        "print(\"Segunda Derivada = \",D2(f(d),h))\n",
        "\n",
        "\n"
      ],
